<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="328dp"
        android:layout_height="288dp"
        app:srcCompat="@drawable/blackimages"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="-11dp"
        android:contentDescription="@string/todois"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/CEO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CEO"
        android:textColor="@color/black"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="298dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tradex_provides_the_means_for_people_to_trade_each_other_whether_students_and_friends_jump_in_and_let_us_trade"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="412dp" />

    <TextView
        android:id="@+id/BONSU ADJEI"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" BONSU ADJEI-ARTHUR"
        android:textColor="@color/black"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="340dp" />
</androidx.constraintlayout.widget.ConstraintLayout>